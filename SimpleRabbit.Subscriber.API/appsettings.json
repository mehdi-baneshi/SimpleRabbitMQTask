{
  "RedisConnection": "localhost:6379",
  "ConnectionStrings": {
    "AppConnString": "Server=(localdb)\\MSSQLLocalDB;Database=SimpleRabbitDB;Trusted_Connection=True;MultipleActiveResultSets=true",
    "MasterConnString": "Server=(localdb)\\MSSQLLocalDB;Database=master;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Application": "Simlpe Rabbit Subscriber API",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ]
  },
  "AllowedHosts": "*"
}
